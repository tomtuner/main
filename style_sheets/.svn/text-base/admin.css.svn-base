BODY {
		background: #E4E4E4;
		font-family: arial;
		font-size: 12px;
		margin-top: 10px;
	}

	/* Structural Elements */

	#page {
		border: 1px solid #CCCCCC;
		background: white;
		margin-left: auto;
		margin-right: auto;
		width: 850px;
		padding: 4px;
	}

	#header {
		background-image: url('../images/headerbg1.jpg');
		background-repeat: repeat-x
		width: 840px;
		height: 48px;
	}

	#navigation {
		background: #DADAD1;
		width: 100%;
		height: 20px;
	}

	#content {
		margin-left: 5px;
		padding: 5px;
		margin-top: 2px;
		float:right;
		height: 80%;
		width: 670px;
		border: 1px dashed #E4E4E4;
	}

	#footer {
		text-align: center;
		background: #DADAD1;
		width: 100%;
		font-size: 14px;
		border-top: 1px solid #ccc;
		margin-top: 10px;
	}

	/* Design Elements */

	.logo {
		position: relative;
		font-family: arial;
		font-size: 20px;
		color: #ffffff;
		font-weight: bold;
		margin-left: 1em;
		top: 10px;
	}

	#user {
		float: right;
		margin-right: 1em;
	}

	#nav {
		background: #DADAD1;
		float: left;
		width: 160px;
	
		margin-top: 2px;
		margin-bottom: 2px;
		
		font-size: 12px;
		
	}

	/* Text */

	h1 {
		font-size: 18px;
		color: #513127;
	}

	h2 {
		font-size: 16px;
		color: #513127;
	}

	b, strong, em {
		color: #513127;
	}

	a, a:visited {
		text-decoration: none;
		color: #000000;
	}

	a:hover {
		text-decoration: underline;
	}

	div.breadcrumbs {
		margin-top: 0.5em;
		font-size: 12px;
	}

	div.breadcrumbs a{
		text-decoration: underline;
	}
	
.location {
 background: #E4E4E4;
}

#web2 {
	position: absolute;
	top: -5px;
	left: 85%;
	width: 99px;
	height: 99px;
	background: url('../images/web2.0.png');
}