.adText{
	font-size:12px;
	font-weight:bold;
	margin:0px;
	padding-left:10px;
}

.adLink{
	font-size:18px;
	font-weight:bold;
	color:#412315;
	margin:0px;
}
a.footerLink { color:#FFFFFF;}
a.specialLink { color:#412315; }

a.specialLink:hover{color:#FFFFFF;}

a.contactLink { color:#412315; text-decoration:none; }

